
<%= form_for @article do |f| %>

<%= f.label :title %>
<%= f.text_field :title %> </br>

<%= f.label :description %>
<%= f.text_field :description %> </br>

<%= f.label :price %>
<%= f.number_field :price %> </br>

<%= f.label :quantity %>
<%= f.number_field :quantity %> </br>

<%= f.submit %>

<% end %>